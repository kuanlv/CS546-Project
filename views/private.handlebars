<h3>Private information</h3>
<div>
    <p>Contact information: {{user.profile.privateInfo.contactInfo}}</p>
</div>
<div>
    <p>My age is: {{user.profile.privateInfo.age}}</p> 
</div>
<div>
    <p>I'm living in {{user.profile.privateInfo.location}}</p> 
</div>
<section>
    {{#each posts}}
        <div class="single-post">
        {{#if this.title}}
            <div class="text-post">
                <p>Post Title: {{ this.title }}</p>
                <article>Post: {{ this.body }}</article>
            </div>
        {{/if}}

        {{#if this.videoTitle}}
            <div class="video-post">
                <p>Video Title: {{ this.videoTitle }}</p>
                <article>Video Description: {{ this.videoDescription }}</article>
                <iframe width="560" height="315" 
                src="{{ this.link }}" 
                frameborder="0" allow="accelerometer; autoplay; 
                encrypted-media; gyroscope; picture-in-picture" 
                allowfullscreen></iframe>
            </div>
        {{/if}}
        </div>
    {{/each}}
</section>